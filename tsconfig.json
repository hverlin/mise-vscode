{
	"compilerOptions": {
		"esModuleInterop": true,
		"isolatedModules": true,
		"lib": ["ES2020"],
		"module": "Preserve",
		"moduleResolution": "Bundler",
		"noEmit": true,
		"noUncheckedIndexedAccess": true,
		"outDir": "out",
		"resolveJsonModule": true,
		"rootDir": "src",
		"sourceMap": true,
		"strict": true,
		"target": "es2022"
	},
	"exclude": ["node_modules", ".vscode-test", "rsbuild.config.ts", "dist"]
}
